{
	"name": "jenssegers/date",
	"version": "5.0.0",
	"description": "A date library to help you work with dates in different languages",
	"keywords": ["laravel", "date", "time", "datetime", "i18n", "translation", "carbon"],
	"homepage": "https://github.com/jenssegers/date",
	"license" : "MIT",
	"minimum-stability": "dev",
	"prefer-stable": true,
	"authors": [
		{
			"name": "Jens Segers",
			"homepage": "https://jenssegers.com"
		},
		{
			"name": "Grzegorz Kisielewicz",
			"homepage": "https://github.com/gkisiel"
		}
	],
	"require": {
		"php": "^8.3",
		"nesbot/carbon": "^3.8.4"
	},
	"require-dev": {
		"phpunit/phpunit": "^11.0",
		"php-coveralls/php-coveralls": "^2.7"
	},
	"autoload": {
		"psr-4": {
			"Jenssegers\\Date\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\Jenssegers\\": "tests/"
		}
	},
	"extra": {
		"laravel": {
			"providers": [
				"Jenssegers\\Date\\DateServiceProvider"
			],
			"aliases": {
				"Date": "Jenssegers\\Date\\Date"
			}
		}
	}
}
